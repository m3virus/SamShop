@model AdminBoothViewModel

<h2>Edit Booth</h2>

<form asp-area="Admin" asp-action="Edit" method="post">
    @Html.AntiForgeryToken()
    <input type="hidden" asp-for="BoothId" />
    <div class="form-group">
        <label asp-for="BoothName">Name:</label>
        <input asp-for="BoothName" class="form-control" />
        <span asp-validation-for="BoothName" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="BoothCreateAt">Create Time:</label>
        <input asp-for="BoothCreateAt" class="form-control" />
       
    </div>


    <div class="form-group">
        <button type="submit" class="btn btn-primary" asp-route-BoothId="@Model.BoothId">Update Booth</button>
    </div>
</form>